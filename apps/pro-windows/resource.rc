	"^G", ID_OPEN_PROJECT
	"^O", ID_OPEN
	"^L", ID_RELOAD
	"^S", ID_SAVE
	"^Q", ID_QUIT
	"^R", ID_CONTINUE
	"^N", ID_NEXT
	"^P", ID_STOP
